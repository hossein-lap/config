c
wmpid(){
	wm_name=${WM}; 	tree="$(ps -o pid= --ppid $$)"; 	pids=$(echo "$tree" | tr ' ' '\n'); 	for pid in $pids; do 		cmd=$(ps -o args= $pid); 		if echo "$cmd" | grep -q "$wm_name"; then 			echo $pid; 			break; 		fi; 	done; }
wmpid(){ 	wm_name=${1}; 	tree="$(ps -o pid= --ppid $$)"; 	pids=$(echo "$tree" | tr ' ' '\n'); 	for pid in $pids; do 		cmd=$(ps -o args= $pid); 		if echo "$cmd" | grep -q "$wm_name"; then 			echo $pid; 			break; 		fi; 	done; }
wmpid dwm
wmpid xinit
